<h1>Your Audio Files:</h1>
<table>
<% @audio_files.each do |audio_file| %>
  <tr>
  <td><%= link_to audio_file.song_title, audio_file_path(audio_file) %></td>
  <td><%= link_to 'Delete', audio_file_path(audio_file),
        method: :delete,
        data: { confirm: 'Do you want to delete this song?' } %></td>
  </tr>
<% end %>
</table>
